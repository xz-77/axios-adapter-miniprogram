{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "composite":true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "removeComments":true,
    "strictNullChecks": true,
    "declaration":true
  }
}
